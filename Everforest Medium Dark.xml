<style-scheme id="everforest-medium-dark" name="Everforest Medium Dark" version="1.0">
  <author>Fabian Dopfer</author>

  <metadata>
    <property name="variant">dark</property>
  </metadata>

  <style name="text"                        foreground="#D3C6AA" background="#2D353B"/>
  <style name="selection"                   background="#543A48"/>
  <style name="search-match"                background="#543A48"/>
  <style name="cursor"                      foreground="#dfdfe0"/>
  <style name="current-line"                background="#343F44"/>
  <style name="current-line-number"         foreground="9DA9A0" background="#343F44"/>
  <style name="line-numbers"                foreground="#7A8478" background="#2D353B"/>
  <style name="draw-spaces"                 foreground="#4f535f"/>
  <style name="background-pattern"          background="#2f2f2f"/>
  <style name="bracket-match"               foreground="#56635f" bold="true"/>
  <style name="bracket-mismatch"            underline="error" bold="true"/>
  <style name="right-margin"                foreground="#9a9a9a" background="#9a9a9a"/>
  <style name="map-overlay"                 background="#9a9a9a"/>
  <style name="def:string"                  foreground="#A7C080"/>
  <style name="def:type"                    foreground="#DBBC7F" bold="true"/>
  <style name="def:comment"                 foreground="#859289"/>
  <style name="def:list-marker"             foreground="#475258" bold="true"/>
  <style name="def:heading"                 bold="true"/>
  <style name="def:identifier"              foreground="#7FBBB3"/>
  <style name="def:preprocessor"            foreground="#D699B6"/>
  <style name="def:keyword"                 foreground="#E67E80" bold="true"/>
  <style name="def:variable"                foreground="#D3C6AA"/>
  <style name="def:special-constant"        foreground="#83C092" bold="true"/>
  <style name="def:emphasis"                bold="true"/>
  <style name="def:strong-emphasis"         italic="true" bold="true"/>
  <style name="def:net-address"             foreground="#7FBBB3"/>
  <style name="def:link-destination"        foreground="#7FBBB3"/>
  <style name="def:link-text"               foreground="#7FBBB3"/>
  <style name="def:floating-point"          foreground="#D699B6"/>
  <style name="def:decimal"                 foreground="#D699B6"/>
  <style name="def:base-n-integer"          foreground="#D699B6"/>
  <style name="def:boolean"                 foreground="#D699B6"/>
  <style name="gtk-doc:function-name"       foreground="#A7C080"/>
  <style name="gtk-doc:function"            foreground="#A7C080"/>
  <style name="gtk-doc:parameter"           bold="true"/>
  <style name="gtk-doc:constant"            bold="true"/>
  <style name="gtk-doc:type"                bold="true"/>
  <style name="gtk-doc:since"               bold="true"/>
  <style name="gtk-doc:return"              bold="true"/>
  <style name="gtk-doc:deprecated"          bold="true"/>
  <style name="gtk-doc:signal-name"         bold="true"/>
  <style name="gtk-doc:property-name"       bold="true"/>

</style-scheme>


